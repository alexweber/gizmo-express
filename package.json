{
  "name": "gizmo-express",
  "version": "0.1.0",
  "description": "Gizmo's Express TypeScript starter",
  "author": "Alex Weber <alexweber15@gmail.com> (http://alexweber.com.br)",
  "license": "Unlicense",
  "private": true,
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "test": "NODE_ENV=test mocha --reporter spec --compilers ts:ts-node/register test/**/*.spec.ts",
    "test:dev": "NODE_ENV=test mocha --watch --reporter spec --compilers ts:ts-node/register test/**/*.spec.ts",
    "cover": "nyc npm test",
    "build": "tsc",
    "build:dev": "tsc --watch",
    "start": "node ./bin/www",
    "start:dev": "DEBUG=gizmo-express:* nodemon --inspect ./bin/www",
    "dev": "yarn build && concurrently --kill-others \"yarn build:dev\" \"yarn start:dev\""
  },
  "dependencies": {
    "apicache": "^0.8.4",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "config": "^1.25.1",
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "helmet": "^3.5.0",
    "mongoose": "^4.8.6",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.8.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.0",
    "@types/chai": "^3.4.35",
    "@types/chai-http": "^0.0.30",
    "@types/config": "^0.0.32",
    "@types/debug": "^0.0.29",
    "@types/errorhandler": "^0.0.30",
    "@types/express-jwt": "^0.0.34",
    "@types/mocha": "^2.2.39",
    "@types/mongoose": "^4.7.8",
    "@types/mongoose-paginate": "^5.0.4",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.8",
    "@types/sinon": "^1.16.35",
    "@types/sinon-chai": "^2.7.27",
    "@types/sinon-stub-promise": "^1.0.27",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "concurrently": "^3.4.0",
    "errorhandler": "^1.5.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "sinon-stub-promise": "^4.0.0",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1"
  }
}
