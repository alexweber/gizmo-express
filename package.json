{
  "name": "gizmo-express",
  "version": "0.1.0",
  "description": "Gizmo's Express TypeScript starter",
  "author": "Alex Weber <alexweber15@gmail.com> (http://alexweber.com.br)",
  "license": "Unlicense",
  "repository": "https://bitbucket.org/alexweber/gizmo-express",
  "private": true,
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "test": "yarn lint && ALLOW_CONFIG_MUTATIONS=1 NODE_ENV=test mocha --reporter spec --compilers ts:ts-node/register test/**/*.spec.ts",
    "test:dev": "ALLOW_CONFIG_MUTATIONS=1 NODE_ENV=test mocha --watch --reporter spec --compilers ts:ts-node/register test/**/*.spec.ts",
    "cover": "nyc yarn test",
    "build": "tsc",
    "build:dev": "tsc --watch",
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development DEBUG=gizmo-express:* nodemon --inspect ./bin/www",
    "dev": "yarn build && concurrently --kill-others \"yarn build:dev\" \"yarn start:dev\"",
    "release": "yarn test && standard-version"
  },
  "dependencies": {
    "acl": "^0.4.10",
    "apicache": "^0.8.4",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "config": "^1.25.1",
    "dompurify": "^0.8.5",
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "helmet": "^3.5.0",
    "jsdom": "^9.12.0",
    "lodash.isobject": "^3.0.2",
    "mongoose": "^4.9.1",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.8.1",
    "redis": "^2.7.1",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "@types/acl": "^0.4.32",
    "@types/body-parser": "^1.16.1",
    "@types/chai": "^3.4.35",
    "@types/chai-http": "^0.0.30",
    "@types/config": "^0.0.32",
    "@types/debug": "^0.0.29",
    "@types/dompurify": "^0.0.29",
    "@types/errorhandler": "^0.0.30",
    "@types/express-jwt": "^0.0.34",
    "@types/lodash.isobject": "^3.0.1",
    "@types/mocha": "^2.2.40",
    "@types/mongoose": "^4.7.8",
    "@types/mongoose-paginate": "^5.0.4",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.8",
    "@types/sinon": "^1.16.36",
    "@types/sinon-chai": "^2.7.27",
    "@types/sinon-stub-promise": "^1.0.27",
    "@types/validator": "^6.2.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "concurrently": "^3.4.0",
    "errorhandler": "^1.5.0",
    "mocha": "^3.2.0",
    "mock-require": "^2.0.1",
    "mockgoose": "^7.0.7",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "sinon-stub-promise": "^4.0.0",
    "standard-version": "^4.0.0",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1"
  }
}
