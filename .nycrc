{
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "bin",
    "config",
    "coverage",
    "dist",
    "node_modules",
    "public",
    "seed",
    "src/**/*.interface.ts"
  ],
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "reporter": [
    "lcov",
    "html",
    "text-summary"
  ],
  "cache": true,
  "all": true,
  "check-coverage": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80
}
